{
    "name": "beelab/user-bundle",
    "type": "symfony-bundle",
    "description": "Simple user provider for Symfony2",
    "keywords": ["User management"],
    "homepage": "http://bee-lab.it",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Massimiliano Arione ",
            "email": "massimiliano.arione@bee-lab.it"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "~2.3",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "sensio/framework-extra-bundle": ">=2.3.0,<4",
        "knplabs/knp-paginator-bundle": "2.4.*",
        "knplabs/knp-components": "~1.2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Beelab\\UserBundle": "" }
    },
    "target-dir": "Beelab/UserBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.x-dev"
        }
    }
}
