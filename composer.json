{
    "name": "beelab/user-bundle",
    "type": "symfony-bundle",
    "description": "Simple user provider for Symfony",
    "keywords": ["User management"],
    "homepage": "https://bee-lab.net",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "massimiliano.arione@bee-lab.net"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "~2.8.12|^3.0",
        "doctrine/orm": "^2.4",
        "doctrine/doctrine-bundle": "^1.2",
        "sensio/framework-extra-bundle": "^2.3|^3.0",
        "twig/twig": ">=1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "knplabs/knp-paginator-bundle": "^2.4",
        "knplabs/knp-components": ">=1.2.4",
        "lexik/form-filter-bundle": "^4.0|^5.0"
    },
    "suggest": {
        "knplabs/knp-paginator-bundle": "Needed for optional pagination",
        "lexik/form-filter-bundle": "If you want to use filters"
    },
    "autoload": {
        "psr-4": { "Beelab\\UserBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2.x-dev"
        }
    }
}
