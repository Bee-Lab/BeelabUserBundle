{
    "name": "beelab/user-bundle",
    "type": "symfony-bundle",
    "description": "Simple user provider for Symfony",
    "keywords": ["User", "User management"],
    "homepage": "https://github.com/Bee-Lab",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "massimiliano.arione@bee-lab.net"
        }
    ],
    "require": {
        "php": "^7.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "sensio/framework-extra-bundle": "^3.0|^4.0|^5.0",
        "symfony/config": "^2.8.12|^3.3|^4.0",
        "symfony/console": "^2.8.12|^3.3|^4.0",
        "symfony/dependency-injection": "^2.8.12|^3.3|^4.0",
        "symfony/doctrine-bridge": "^2.8.12|^3.3|^4.0",
        "symfony/form": "^2.8.12|^3.3|^4.0",
        "symfony/security": "^2.8.12|^3.3|^4.0",
        "symfony/validator": "^2.8.12|^3.3|^4.0",
        "twig/twig": ">=1.26"
    },
    "require-dev": {
        "knplabs/knp-components": ">=1.2.4",
        "knplabs/knp-paginator-bundle": "^2.5",
        "lexik/form-filter-bundle": "^5.0",
        "phpunit/phpunit": "^6.2|^7.0"
    },
    "suggest": {
        "knplabs/knp-paginator-bundle": "Needed for optional pagination",
        "lexik/form-filter-bundle": "If you want to use filters"
    },
    "autoload": {
        "psr-4": { "Beelab\\UserBundle\\": "" }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4.x-dev"
        }
    }
}
